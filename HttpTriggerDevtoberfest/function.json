{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "devtoberfest/marvel/{id}",
      "methods": [
        "get"
      ]
    },
    {
      "type": "blob",
      "direction": "in",
      "name": "config",
      "path": "marvelcharacters/marvel-character-{id}.json"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "scriptFile": "../dist/HttpTriggerDevtoberfest/index.js"
}