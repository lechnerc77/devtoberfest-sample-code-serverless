{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "devtoberfest/staging/marvel/",
      "methods": [
        "post"
      ]
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "stage",
      "path": "staging/marvel-character-{id}-{nickName}.json"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "scriptFile": "../dist/HttpTriggerDevtoberfest/index.js"
}